CLEAN=""
TESTS=
  echo 'cat << Makefile < Makefile'
[19-END]


[20]
NAME="cat >> .m > .lol"
SETUP=""
CLEAN=""
TESTS=
  echo 'cat >> .m > .lol'
[20-END]


[21]
NAME="cat << Makefile >> .lol"
SETUP=""
CLEAN=""
TESTS=
  echo 'cat Makefile > .lol'
  echo 'cat << Makefile >> .lol'
  echo 'cat .lol'
  echo 'rm .lol'
[21-END]


[22]
NAME="env > .lol"
SETUP=""
CLEAN=""
TESTS=
  echo 'unsetenv SHLVL'
  echo 'unsetenv HOST'
  echo 'unsetenv _'
  echo 'unsetenv HOSTTYPE'
  echo 'unsetenv VENDOR'
  echo 'unsetenv OSTYPE'
  echo 'unsetenv MACHTYPE'
  echo 'unsetenv GROUP'
  echo 'env > .lol'
  echo 'cat .lol'
  echo 'rm .lol'
[22-END]

[23]
NAME="ls | cat < Makefile > .lol"
SETUP=""
CLEAN=""
TESTS=
  echo 'ls | cat < Makefile > .lol'
[23-END]

[24]
NAME="ls | cat -e > .lol"
SETUP=""
CLEAN=""
TESTS=
  echo 'ls | cat -e > .lol'
  echo 'cat .lol'
  echo 'rm .lol'
[24-END]

[25]
NAME="cat < Makefile | cat -e > .lol"
SETUP=""
CLEAN=""
TESTS=
  echo 'cat < Makefile | cat -e > .lol'
  echo 'cat .lol'
  echo 'rm .lol'
[25-END]

