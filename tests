SETUP=""
CLEAN=""
TESTS=
  echo 'cat < Makefile'
[15-END]

[16]
NAME="<Makefile cat|> .lol wc -l "
SETUP=""
CLEAN=""
TESTS=
  echo '< Makefile cat|>.lol wc -l'
  echo 'cat .lol'
  echo 'rm .lol'
[16-END]

[17]
NAME="cat << a"
SETUP=""
CLEAN=""
TESTS=
